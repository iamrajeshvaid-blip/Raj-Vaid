<svg width="512" height="512" viewBox="0 0 512 512" xmlns="http://www.w3.org/2000/svg">
  <!-- Dark background -->
  <rect width="512" height="512" fill="#0f172a"/>
  
  <!-- Outer glow -->
  <defs>
    <filter id="glow">
      <feGaussianBlur stdDeviation="4" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>
  
  <!-- Wolf head shape (simplified geometric) -->
  <g filter="url(#glow)">
    <!-- Main head -->
    <circle cx="256" cy="280" r="120" fill="#1e293b" stroke="#00d9ff" stroke-width="2"/>
    
    <!-- Snout -->
    <ellipse cx="256" cy="330" rx="70" ry="60" fill="#1e293b" stroke="#00d9ff" stroke-width="2"/>
    
    <!-- Left ear -->
    <polygon points="200,180 170,100 210,160" fill="#1e293b" stroke="#00d9ff" stroke-width="2"/>
    
    <!-- Right ear -->
    <polygon points="312,180 342,100 302,160" fill="#1e293b" stroke="#00d9ff" stroke-width="2"/>
    
    <!-- Left eye (glowing cyan) -->
    <circle cx="230" cy="260" r="25" fill="#00d9ff" stroke="#0ea5e9" stroke-width="2"/>
    <circle cx="230" cy="260" r="12" fill="#000" opacity="0.6"/>
    <circle cx="235" cy="255" r="6" fill="#00d9ff"/>
    
    <!-- Right eye (glowing cyan) -->
    <circle cx="282" cy="260" r="25" fill="#00d9ff" stroke="#0ea5e9" stroke-width="2"/>
    <circle cx="282" cy="260" r="12" fill="#000" opacity="0.6"/>
    <circle cx="287" cy="255" r="6" fill="#00d9ff"/>
    
    <!-- Nose -->
    <ellipse cx="256" cy="310" rx="15" ry="12" fill="#0ea5e9"/>
    
    <!-- Mouth line -->
    <path d="M 256 310 Q 240 330 225 335" stroke="#00d9ff" stroke-width="3" fill="none" stroke-linecap="round"/>
    <path d="M 256 310 Q 272 330 287 335" stroke="#00d9ff" stroke-width="3" fill="none" stroke-linecap="round"/>
  </g>
  
  <!-- Accent glow around head -->
  <circle cx="256" cy="280" r="130" fill="none" stroke="#a855f7" stroke-width="1" opacity="0.5"/>
</svg>